<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practicas.janhout.practicacontentprovider">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Principal"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.practicas.janhout.practicacontentprovider.DatosInmuebles"
            android:label="@string/title_activity_datos_inmuebles" >
        </activity>
        <activity
            android:name="com.practicas.janhout.practicacontentprovider.Editar"
            android:label="@string/title_activity_editar" >
        </activity>

        <provider
            android:name=".basedatos.ProveedorInmueble"
            android:authorities="com.practicas.janhout.practicacontentprovider.basedatos.proveedorinmueble"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>
